public class IsPrime {
    public static void main(String[] args) {
        int num = 29;
        boolean IsPrime = true;
        for (int i = 2; i < num; i++) {
            if (num % i == 0) {
                IsPrime = false;
                break;
            }
        }

        if (IsPrime)
            System.out.println(num + " is a prime number.");
        else
            System.out.println(num + " is not a prime number.");

    }
}
